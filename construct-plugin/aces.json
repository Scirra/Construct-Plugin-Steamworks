{
	"steamworks": {
		"conditions": [
			{
				"id": "is-available",
				"scriptName": "IsAvailable"
			}, {
				"id": "is-running-on-steam-deck",
				"scriptName": "IsRunningOnSteamDeck"
			}, {
				"id": "on-overlay-shown",
